{% extends 'base.html.twig' %}

{% block title %}Hello ArticleController!{% endblock %}

{% block body %}

  {% for article in articles %}
      <h2>{{ article.title }}</h2>
      <p>{{ article.content }}</p>
      <img src="{{ asset('img/' ~ article.photo) }}" alt="Photo de l'article">

  {% endfor %}


{% endblock %}
